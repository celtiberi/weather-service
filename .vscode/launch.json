{
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "autoReload": {
        "enable": true
      },
      "envFile": "${workspaceFolder}/weather-routing-service/dev.env"
    }
  ]
}
